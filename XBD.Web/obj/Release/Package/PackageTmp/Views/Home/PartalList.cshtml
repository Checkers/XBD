@{
    var conf = System.Configuration.ConfigurationManager.AppSettings["WebTypeCode"];
    var w = XBD.Web.Utilities.WebInfo.GetWebInfo();
    var listNav = new XBD.Service.CategorySvc().GetWebTypeByCodeName(conf).Where(t=>t.Name != "首页" ).Take(9);
}
<div class="col-lg-4" com-left>

    <div  com-left-item>
        @{var index = 3;}
        @foreach (var item in listNav)
        {
            
            <a href="@item.Link" class="col-lg-4" com-left-item="@(index++)">@item.Name</a>
        }
        @*<a href="/about" class="col-lg-6" com-left-item="66">联系我们</a>*@
    </div>
    <div about-us>
        关于我们<br />
        <p img-sh></p>
        联系方式<br />
        <p>
            手机：@w.UserPhone1 、@w.UserPhone2<br />
            座机：@w.TelCall<br />
            QQ号：@w.QQ<br />
            邮箱：@(w.Email)<br />
            地址：@w.Address 
        </p>
    </div>
</div>